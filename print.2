#include "main.h"
#include <stdio.h>
#include <stdarg.h>

int _printf(const char *format, ...)
{
    va_list args;
    va_start(args, format);

    int count = vprintf(format, args);

    va_end(args);
    return count;
}

int main()
{
    int count = _printf("The answer is %d or %i\n", 42);

    printf("Characters printed: %d\n", count);

    return 0;
}
