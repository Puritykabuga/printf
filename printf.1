#include <stdio.h>
#include <stdarg.h>
#include <stddef.h>

void print_buffer(char buffer[], int *buff_ind);i

/**
 * _printf - Printf function
 * @format: format.
 * Return: Printed chars.
 */

int _printf(const char *format, ...)
{
    va_list args;
    va_start(args, format);

    int count = vfprintf(stdout, format, args);

    va_end(args);
    return count;
}

int main()
{
    int count = _printf("Hello %s, your lucky number is %d%c\n", "John", 7, '!');

    printf("Characters printed: %d\n", count);

    return 0;
}
